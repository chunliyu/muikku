{#events}
  {@select key="{type}"}
    {@eq value="EVALUATION_REQUEST"}
      <div class="eval-modal-workspace-event" data-identifier="{identifier}" data-type="EVALUATION_REQUEST">
        <div class="eval-modal-workspace-event-header">
          <div class="eval-modal-workspace-event-date">{date|formatDate}</div>
          <div class="eval-modal-workspace-event-details"><span class="eval">Arviointipyyntö</span> opiskelijalta <span class="author">{student}</span></div>
        </div>
        <div class="eval-modal-workspace-event-content">
          <div class="workspace-event-content-toggle"><span class="icon-arrow-down-thin"></span><span>Arviointipyyntö</span></div>
          <div class="workspace-event-content-text" style="display: none;">{text|s}</div>
        </div>
        <div class="eval-modal-workspace-event-buttonset">
          <div class="eval-modal-workspace-event-button button-edit-event">Muokkaa</div>
          <div class="eval-modal-workspace-event-button button-remove-event">Poista</div>
        </div>
      </div>
    {/eq}
    {@eq value="SUPPLEMENTATION_REQUEST"}
      <div class="eval-modal-workspace-event" data-identifier="{identifier}" data-type="SUPPLEMENTATION_REQUEST">
        <div class="eval-modal-workspace-event-header">
          <div class="eval-modal-workspace-event-date">{date|formatDate}</div>
          <div class="eval-modal-workspace-event-details"><span class="eval">Täydennyspyyntö</span> opettajalta <span class="author">{author}</span></div>
          <div class="eval-modal-workspace-event-grade">T</div>
        </div>
        <div class="eval-modal-workspace-event-buttonset">
          <div class="eval-modal-workspace-event-button button-edit-event">Muokkaa</div>
          <div class="eval-modal-workspace-event-button button-remove-event">Poista</div>
        </div>
      </div>
    {/eq}
    {@eq value="EVALUATION_FAIL"}
      <div class="eval-modal-workspace-event" data-identifier="{identifier}" data-type="EVALUATION_FAIL">
        <div class="eval-modal-workspace-event-header">
          <div class="eval-modal-workspace-event-date">{date|formatDate}</div>
          <div class="eval-modal-workspace-event-details">Opettaja <span class="author">{author}</span> arvioi kurssisuorituksen <span class="eval">hylätyksi</span></div>
          <div class="eval-modal-workspace-event-grade">{grade}</div>
        </div>
        <div class="eval-modal-workspace-event-buttonset">
          <div class="eval-modal-workspace-event-button button-edit-event">Muokkaa</div>
          <div class="eval-modal-workspace-event-button button-remove-event">Poista</div>
        </div>
      </div>
    {/eq}
    {@eq value="EVALUATION_PASS"}
      <div class="eval-modal-workspace-event" data-identifier="{identifier}" data-type="EVALUATION_PASS">
        <div class="eval-modal-workspace-event-header">
          <div class="eval-modal-workspace-event-date">{date|formatDate}</div>
          <div class="eval-modal-workspace-event-details">Opettaja <span class="author">{author}</span> arvioi kurssisuorituksen <span class="eval">hyväksytyksi</span></div>
          <div class="eval-modal-workspace-event-grade">{grade}</div>
        </div>
        <div class="eval-modal-workspace-event-buttonset">
          <div class="eval-modal-workspace-event-button button-edit-event">Muokkaa</div>
          <div class="eval-modal-workspace-event-button button-remove-event">Poista</div>
        </div>
      </div>
    {/eq}
  {/select}
{/events}

