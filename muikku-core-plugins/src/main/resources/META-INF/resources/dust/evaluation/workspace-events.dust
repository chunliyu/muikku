{#events}
  <div class="eval-modal-workspace-event" data-identifier="{identifier}" data-type="{type}">
    <div class="eval-modal-workspace-event-header">
      <div class="eval-modal-workspace-event-date">{date|formatDate}</div>
      <div class="eval-modal-workspace-event-details">{author}</div>
      {?grade}<div class="eval-modal-workspace-event-grade">{grade}</div>{/grade}
    </div>
    {!<div class="eval-modal-workspace-event-data">{text|s}</div>!}
    {@ne key=type value=EVALUATION_REQUEST}
      <div class="eval-modal-workspace-event-buttonset">
        <div class="eval-modal-workspace-event-button button-edit-event">Muokkaa</div>
        <div class="eval-modal-workspace-event-button button-remove-event">Poista</div>
      </div>
    {/ne}
  </div>
{/events}

