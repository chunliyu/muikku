<div class="communicator item-list communicator-item-list-navigation">
  {#items currentLocation=currentLocation}
    <a class="item-list-item {@eq value=currentLocation key=location}active{/eq}" href="#{location}">
      <span class="icon icon-{icon}" {?color}style="color:{color}"{/color}></span>
      <span class="item-list-text-body text">
        {text}
      </span>
    </a>
  {/items}
</div>