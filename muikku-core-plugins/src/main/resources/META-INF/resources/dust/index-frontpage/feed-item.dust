<ul class="feed">
  {#entries}
    <li class="feed-item">
      <span class="feed-item-description">
        <a href="{link|s}" target="top">{title}</a>
        {description|s}
      </span>
      <span class="feed-item-date">{@moment type="format" date="{publicationDate}" format="L"/}</span>
    </li>
  {/entries}
</ul>