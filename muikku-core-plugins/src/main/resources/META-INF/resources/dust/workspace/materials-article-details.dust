{?producers}
  <div class="producers-override">
    <span class="material-producers-label">{#localize key="plugin.workspace.materials.producersLabel"/}:</span>
    {#producers}
      <span class="material-producer">{.|s}{@sep}, {/sep}</span>
    {/producers}
  </div>
{/producers}

{?license}
  <div class="license-override">
    <span class="material-license-label">{#localize key="plugin.workspace.materials.licenseLabel"/}:</span>
    <span class="material-license">{?licenseIsUrl}<a href="{license}" target="top">{license}</a>{:else}{license}{/licenseIsUrl}</span>
  </div>
{/license}